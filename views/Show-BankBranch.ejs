<%- include('header') %>

<div class="main-panel">
    <div class="content-wrapper">
      <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
          <div class="card">
            <div class="card-body">
              <h4 class="card-title">Admin Details</h4>
              <div class="table-responsive">
                <table class="table table-striped">
                  <thead>
                    <tr>
                      <th>
                        No.
                      </th>
                      <th>
                        Bank Name
                      </th>
                      <th>
                        Bank Branch
                      </th>
                      <th>
                        Branch IFSC Code
                      </th>
                      <th>
                        Action 
                      </th>
                    </tr>
                  </thead>
                  <tbody>
                    <% var i = 1 %>
                    <% for (var BB of BankBranch) { %>
                    <tr>
                      <td>
                        <%= i++ %>
                      </td>
                      <td>
                        <%= BB.BankNameID.BankName %>
                      </td>
                      <td>
                        <%= BB.BankBranch %>
                      </td>
                      <td>
                        <%= BB.IFSCcode %>
                      </td>
                      <td>
                        <a href="/DeleteData/<%= BB.id %>">DeleteData</a> ||
                        <a href="/UpdateDataPage/<%= BB.id %>">UpdateData</a>
                      </td>
                    </tr>
                    <% } %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

<%- include('footer') %>